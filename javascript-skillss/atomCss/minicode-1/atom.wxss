.at-line-bottom-after::after {
  content: "";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  border-bottom: 2rpx solid red;
  transform-origin: 0 100%;
  transform: scaleY(0.5);
}
.pt-36 {
  padding-top: 36rpx;
}
.pt-40 {
  padding-top: 40rpx;
}
.border-none {
  border: none;
}
.border-0 {
  border: 0;
}
.border-unset {
  border: unset;
}
.border-style-none {
  border-style: none;
}
.position-relative {
  position: relative;
}
